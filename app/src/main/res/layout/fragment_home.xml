<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context=".ui.fragment.home.HomeFragment">    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="match_parent">        <androidx.recyclerview.widget.RecyclerView            android:id="@+id/rv_category_list"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:visibility="gone"/>        <ExpandableListView            android:id="@+id/elv_category_list"            android:layout_height="match_parent"            android:layout_width="match_parent"            android:groupIndicator="@null"            android:clipToPadding="false"            android:divider="@null"            android:dividerHeight="@null"            android:childDivider="@null"            android:footerDividersEnabled="true"            tools:listitem="@layout/layout_category_item"/>    </RelativeLayout></androidx.constraintlayout.widget.ConstraintLayout>
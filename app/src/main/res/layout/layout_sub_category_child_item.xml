<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:paddingEnd="@dimen/margin_small"    android:paddingStart="@dimen/margin_small"    android:paddingBottom="@dimen/margin_small"    android:background="@color/colorListBg">    <androidx.cardview.widget.CardView        android:layout_width="match_parent"        android:layout_height="wrap_content"        app:cardCornerRadius="@dimen/card_corner_radius_2"        app:cardElevation="@dimen/card_elevation_2"        app:cardUseCompatPadding="true">        <androidx.recyclerview.widget.RecyclerView            android:id="@+id/rv_sub_category"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:clipToPadding="false"/>    </androidx.cardview.widget.CardView></RelativeLayout>